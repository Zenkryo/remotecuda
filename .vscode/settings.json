{
  "files.exclude": {
    "**/*.o": true,
    "**/*.so": true,
    "**/*.a": true,
    "**/server": true,
    "**/*.swp": true,
  },
  "C_Cpp.default.includePath": [
    "/usr/local/cuda/include",
    "/usr/local/cudnn/include",
    "${workspaceFolder}",
  ],
  "files.associations": {
    "cstdint": "cpp",
    "array": "cpp",
    "atomic": "cpp",
    "bit": "cpp",
    "*.tcc": "cpp",
    "bitset": "cpp",
    "cctype": "cpp",
    "chrono": "cpp",
    "clocale": "cpp",
    "cmath": "cpp",
    "codecvt": "cpp",
    "condition_variable": "cpp",
    "cstdarg": "cpp",
    "cstddef": "cpp",
    "cstdio": "cpp",
    "cstdlib": "cpp",
    "cstring": "cpp",
    "ctime": "cpp",
    "cwchar": "cpp",
    "cwctype": "cpp",
    "deque": "cpp",
    "unordered_map": "cpp",
    "vector": "cpp",
    "exception": "cpp",
    "algorithm": "cpp",
    "functional": "cpp",
    "iterator": "cpp",
    "memory": "cpp",
    "memory_resource": "cpp",
    "numeric": "cpp",
    "optional": "cpp",
    "random": "cpp",
    "ratio": "cpp",
    "string": "cpp",
    "string_view": "cpp",
    "system_error": "cpp",
    "tuple": "cpp",
    "type_traits": "cpp",
    "utility": "cpp",
    "fstream": "cpp",
    "initializer_list": "cpp",
    "iomanip": "cpp",
    "iosfwd": "cpp",
    "iostream": "cpp",
    "istream": "cpp",
    "limits": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "ostream": "cpp",
    "sstream": "cpp",
    "stdexcept": "cpp",
    "streambuf": "cpp",
    "thread": "cpp",
    "typeinfo": "cpp",
    "__nullptr": "cpp",
    "map": "cpp",
    "set": "cpp",
    "cinttypes": "cpp",
    "__debug": "cpp",
    ".clang-format": "clang-format"
  },
  "C_Cpp.default.defines": [
    "CUBLASAPI=",
  ],
  "C_Cpp.default.compilerPath": "/usr/bin/g++",
  "C_Cpp.formatting": "clangFormat",
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true,
  },
  "black-formatter.args": [
    "--line-length",
    "256"
  ],
  "editor.formatOnSave": true,
  "[clang-format]": {
    "editor.formatOnSave": false
  },
  "files.trimTrailingWhitespace": true,
  "files.insertFinalNewline": true
}
